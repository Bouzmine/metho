{
  "name": {
    "fr": "APA",
    "en": "APA"
  },
  "translatedIn": [
    "fr",
    "en"
  ],
  "templates": [
    {
      "name": {
        "fr": "Livre",
        "en": "Book"
      },
      "translatedIn": [
        "fr",
        "en"
      ],
      "fields": [
        {
          "type": "authors",
          "transform": "firstLetterDotAfterLastName",
          "token": "authors",
          "repeatUpTo": 5,
          "multipleAuthorsAmpersand": "&",
          "moreThan": {
            "number": 5,
            "cite": 1,
            "expression": "et al."
          }
        },
        {
          "type": "authors",
          "transform": "firstLetterDotAfterLastName",
          "token": "translators",
          "repeatUpTo": 3,
          "multipleAuthorsAmpersand": "&",
          "moreThan": {
            "number": 5,
            "cite": 1,
            "expression": "et al."
          }
        },
        {
          "type": "publicationYear",
          "token": "publicationYear"
        },
        {
          "type": "title",
          "name": {
            "fr": "Titre du livre",
            "en": "Title of the book"
          }
        },
        {
          "type": "publicationLocation",
          "token": "publicationLocation"
        },
        {
          "type": "publisher",
          "name": {
            "fr": "Éditeur",
            "en": "Publisher"
          },
          "token": "publisher"
        }
      ],
      "compile": {
        "if": [
          {
            "with": ["translators"],
            "without": [],
            "compileString": "{{authors}}. ({{publicationYear}}). <i>{{title}}</i>. ({{translators}}, Trans.). {{publicationLocation}}: {{publisher}}."
          }
        ],
        "default": "{{authors}}. ({{publicationYear}}). <i>{{title}}</i>. {{publicationLocation}}: {{publisher}}."
      }
    },
    {
      "name": {
        "fr": "Article de périodique",
        "en": "Journal Article"
      },
      "translatedIn": [
        "fr",
        "en"
      ],
      "fields": [
        {
          "type": "authors",
          "transform": "firstLetterDotAfterLastName",
          "token": "authors",
          "repeatUpTo": 5,
          "multipleAuthorsAmpersand": "&",
          "moreThan": {
            "number": 5,
            "cite": 1,
            "expression": "et al."
          }
        },
        {
          "type": "publicationYear",
          "token": "publicationYear",
          "default": "n.d."
        },
        {
          "type": "text",
          "token": "articleTitle",
          "name": {
            "fr": "Titre de l'article",
            "en": "Article itle"
          }
        },
        {
          "type": "text",
          "token": "journalTitle",
          "name": {
            "fr": "Nom du périodique",
            "en": "Journal title"
          }
        },
        {
          "type": "number",
          "token": "volumeNumber",
          "name": {
            "fr": "Numéro de volume",
            "en": "Volume number"
          }
        },
        {
          "type": "number",
          "token": "issueNumber",
          "name": {
            "fr": "Numéro de parution",
            "en": "Issue number"
          }
        },
        {
          "type": "url",
          "name": {
            "fr": "Adresse web d'origine",
            "en": "Retrieved from web address"
          },
          "token": "url"
        },
        {
          "type": "number",
          "token": "startPage",
          "name": {
            "fr": "Page de début",
            "en": "Start page"
          }
        },
        {
          "type": "number",
          "token": "endPage",
          "name": {
            "fr": "Page de fin",
            "en": "End page"
          }
        }
      ],
      "compile": {
        "if": [
          {
            "with": ["volumeNumber", "issueNumber", "url"],
            "without": ["startPage", "endPage"],
            "compileString": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{volumeNumber}}({{issueNumber}}). Retrieved from {{url}}"
          },
          {
            "with": ["startPage", "endPage"],
            "without": ["volumeNumber", "issueNumber", "url"],
            "compileString": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{startPage}}-{{endPage}}."
          },
          {
            "with": ["startPage", "endPage", "url"],
            "without": ["volumeNumber", "issueNumber"],
            "compileString": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{startPage}}-{{endPage}}. Retrieved from {{url}}"
          },
          {
            "with": ["volumeNumber", "issueNumber"],
            "without": ["url", "startPage", "endPage"],
            "compileString": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{volumeNumber}}({{issueNumber}})."
          },
          {
            "with": ["volumeNumber"],
            "without": ["issueNumber", "url", "startPage", "endPage"],
            "compileString": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{volumeNumber}}."
          },
          {
            "with": ["volumeNumber", "issueNumber"],
            "without": ["url", "startPage", "endPage"],
            "compileString": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{volumeNumber}}({{issueNumber}})."
          }
        ],
        "default": "{{authors}}. ({{publicationYear}}). {{articleTitle}}. <i>{{journalTitle}}</i>, {{volumeNumber}}({{issueNumber}}), {{startPage}}-{{endPage}}. Retrieved from {{url}}"
      }
    },
    {
